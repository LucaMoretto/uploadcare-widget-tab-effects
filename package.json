{
  "name": "uploadcare-widget-tab-effects",
  "config": {
    "name": "uploadcare.tab-effects",
    "library": "uploadcareTabEffects",
    "classPrefix": "uploadcare-tab-effects--"
  },
  "version": "1.0.0-alpha.3",
  "description": "The Effects Tab for Uploadcare Widget",
  "main": "dist/uploadcare.tab-effects.min.js",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint .",
    "build:dist": "rollup -c ./scripts/rollup.config.js",
    "build:dist:min": "rollup -c ./scripts/rollup.config.js --environment BUILD:production",
    "build": "npm run clean && npm run build:dist && npm run build:dist:min",
    "start:static": "npm run build && browser-sync start --ss dist --files dist --port 3126",
    "start": "npm run build && browser-sync start -s demo --ss dist --files demo dist --port 3127",
    "publish:cdn": "node ./scripts/publish-to-cdn.js"
  },
  "dependencies": {},
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "aws-sdk": "^2.22.0",
    "babel-eslint": "^6.1.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-stage-3": "^6.22.0",
    "browser-sync": "^2.11.1",
    "cssnano": "^3.10.0",
    "eslint": "^2.13.1",
    "eslint-config-uploadcare": "^1.1.0",
    "postcss-modules": "^0.6.4",
    "postcss-nested": "^1.0.0",
    "postcss-reporter": "^3.0.0",
    "rimraf": "^2.5.4",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-filesize": "^1.2.1",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-license": "^0.3.0",
    "rollup-plugin-postcss": "^0.4.1",
    "rollup-plugin-posthtml-template": "^1.1.0",
    "rollup-plugin-uglify": "^1.0.1",
    "stylelint": "^7.6.0",
    "stylelint-config-uploadcare": "^1.0.0"
  },
  "browserslist": [
    "> .4%",
    "ie >= 10"
  ],
